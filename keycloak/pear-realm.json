        {
          "name": "tickets:assign",
          "description": "Assign tickets to agents"
        }
      ],
      "stores-api": [
        {
          "name": "stores:read",
          "description": "View store information"
        },
        {
          "name": "stores:write",
          "description": "Manage store information"
        },
        {
          "name": "stores:appointments",
          "description": "Manage store appointments"
        }
      ]
    }
