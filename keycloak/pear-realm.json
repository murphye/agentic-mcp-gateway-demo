  "groups": [
    {
      "name": "Customers",
      "path": "/Customers",
      "attributes": {
        "userType": ["customer"]
      },
      "realmRoles": ["customer"],
      "clientRoles": {
        "customer-accounts-api": ["accounts:read:own", "accounts:write:own"],
        "payments-api": ["payments:read:own", "payments:write:own"],
        "orders-api": ["orders:read:own", "orders:write:own"],
        "support-api": ["tickets:read:own", "tickets:write:own"],
        "product-catalog-api": ["products:read"],
        "stores-api": ["stores:read"]
      },
      "subGroups": [
        {
          "name": "Premium",
          "path": "/Customers/Premium",
          "attributes": {
            "tier": ["premium"],
            "pearOneSubscriber": ["true"]
          },
          "realmRoles": ["premium_customer"]
        },
        {
          "name": "Standard",
          "path": "/Customers/Standard",
          "attributes": {
            "tier": ["standard"]
          }
        },
        {
          "name": "Family-Organizers",
          "path": "/Customers/Family-Organizers",
          "attributes": {
            "familySharing": ["organizer"]
          },
          "realmRoles": ["family_organizer"]
        },
        {
          "name": "Developer-Program",
          "path": "/Customers/Developer-Program",
          "attributes": {
            "developerAccess": ["true"]
          },
          "realmRoles": ["developer"]
        }
      ]
    },
    {
      "name": "Employees",
      "path": "/Employees",
      "attributes": {
        "userType": ["employee"]
      },
      "realmRoles": ["employee"],
      "clientRoles": {
        "customer-accounts-api": ["accounts:read:all"],
        "orders-api": ["orders:read:all"],
        "product-catalog-api": ["products:read"],
        "stores-api": ["stores:read"]
      },
      "subGroups": [
        {
          "name": "Corporate",
          "path": "/Employees/Corporate",
          "attributes": {
            "department": ["corporate"]
          },
          "subGroups": [
            {
              "name": "Executives",
              "path": "/Employees/Corporate/Executives",
              "attributes": {
                "level": ["executive"]
              },
              "realmRoles": ["admin"],
              "clientRoles": {
                "analytics-api": ["analytics:read", "analytics:reports:read", "analytics:reports:write", "analytics:query"]
              }
            },
            {
              "name": "Engineering",
              "path": "/Employees/Corporate/Engineering",
              "attributes": {
                "department": ["engineering"]
              },
              "clientRoles": {
                "product-catalog-api": ["products:write"]
              }
            },
            {
              "name": "Support",
              "path": "/Employees/Corporate/Support",
              "attributes": {
                "department": ["support"]
              },
              "realmRoles": ["support_agent"],
              "clientRoles": {
                "support-api": ["tickets:read:all", "tickets:write:all", "tickets:assign"],
                "orders-api": ["orders:cancel"]
              }
            },
            {
              "name": "Analytics",
              "path": "/Employees/Corporate/Analytics",
              "attributes": {
                "department": ["analytics"]
              },
              "realmRoles": ["analytics_admin"],
              "clientRoles": {
                "analytics-api": ["analytics:read", "analytics:reports:read", "analytics:reports:write", "analytics:query"]
              }
            },
            {
              "name": "Operations",
              "path": "/Employees/Corporate/Operations",
              "attributes": {
                "department": ["operations"]
              },
              "realmRoles": ["inventory_manager", "order_manager"],
              "clientRoles": {
                "inventory-api": ["inventory:read", "inventory:write", "inventory:transfer", "inventory:adjust"],
                "orders-api": ["orders:write:all", "orders:fulfill"]
              }
            }
          ]
        },
        {
          "name": "Retail",
          "path": "/Employees/Retail",
          "attributes": {
            "department": ["retail"]
          },
          "clientRoles": {
            "inventory-api": ["inventory:read"],
            "stores-api": ["stores:appointments"]
          },
          "subGroups": [
            {
              "name": "Store-Managers",
              "path": "/Employees/Retail/Store-Managers",
              "attributes": {
                "role": ["manager"]
              },
              "realmRoles": ["store_manager"],
              "clientRoles": {
                "inventory-api": ["inventory:write", "inventory:transfer"],
                "stores-api": ["stores:write"],
                "orders-api": ["orders:write:all", "orders:cancel"],
                "payments-api": ["payments:refund"],
                "analytics-api": ["analytics:read", "analytics:reports:read"]
              }
            },
            {
              "name": "Sales-Associates",
              "path": "/Employees/Retail/Sales-Associates",
              "attributes": {
                "role": ["sales"]
              },
              "realmRoles": ["sales_associate"],
              "clientRoles": {
                "payments-api": ["payments:process"],
                "orders-api": ["orders:write:all"]
              }
            },
            {
              "name": "Genius-Bar",
              "path": "/Employees/Retail/Genius-Bar",
              "attributes": {
                "role": ["technical_support"]
              },
              "realmRoles": ["support_agent"],
              "clientRoles": {
                "support-api": ["tickets:read:all", "tickets:write:all"]
              }
            }
          ]
        }
      ]
    }
