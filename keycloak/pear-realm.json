  "roles": {
    "realm": [
      {
        "name": "customer",
        "description": "Base customer role - can view own orders, manage own account",
        "composite": false
      },
      {
        "name": "premium_customer",
        "description": "Premium/Pear One subscriber with enhanced benefits",
        "composite": true,
        "composites": {
          "realm": ["customer"]
        }
      },
      {
        "name": "family_organizer",
        "description": "Family sharing group organizer",
        "composite": true,
        "composites": {
          "realm": ["customer"]
        }
      },
      {
        "name": "developer",
        "description": "Pear Developer Program member",
        "composite": true,
        "composites": {
          "realm": ["customer"]
        }
      },
      {
        "name": "employee",
        "description": "Base employee role - internal Pear Computer staff",
        "composite": false
      },
      {
        "name": "support_agent",
        "description": "Customer support agent - can view customer info and tickets",
        "composite": true,
        "composites": {
          "realm": ["employee"]
        }
      },
      {
        "name": "sales_associate",
        "description": "Retail sales associate - can process sales and view inventory",
        "composite": true,
        "composites": {
          "realm": ["employee"]
        }
      },
      {
        "name": "store_manager",
        "description": "Retail store manager - full store operations access",
        "composite": true,
        "composites": {
          "realm": ["employee", "sales_associate"]
        }
      },
      {
        "name": "inventory_manager",
        "description": "Inventory manager - can manage stock levels and transfers",
        "composite": true,
        "composites": {
          "realm": ["employee"]
        }
      },
      {
        "name": "analytics_viewer",
        "description": "Can view analytics dashboards and reports",
        "composite": true,
        "composites": {
          "realm": ["employee"]
        }
      },
      {
        "name": "analytics_admin",
        "description": "Can create and manage analytics reports",
        "composite": true,
        "composites": {
          "realm": ["employee", "analytics_viewer"]
        }
      },
      {
        "name": "order_manager",
        "description": "Can manage and process all orders",
        "composite": true,
        "composites": {
          "realm": ["employee"]
        }
      },
      {
        "name": "admin",
        "description": "System administrator with full access",
        "composite": true,
        "composites": {
          "realm": ["employee", "store_manager", "inventory_manager", "analytics_admin", "order_manager", "support_agent"]
        }
      }
    ],
    "client": {
      "customer-accounts-api": [
        {
          "name": "accounts:read:own",
          "description": "Read own account data"
        },
        {
          "name": "accounts:write:own",
          "description": "Modify own account data"
        },
        {
          "name": "accounts:read:all",
          "description": "Read all customer accounts (employees only)"
        },
        {
          "name": "accounts:write:all",
          "description": "Modify any customer account (admin only)"
        }
      ],
      "payments-api": [
        {
          "name": "payments:read:own",
          "description": "Read own payment methods"
        },
        {
          "name": "payments:write:own",
          "description": "Manage own payment methods"
        },
        {
          "name": "payments:read:all",
          "description": "Read all payment data (employees only)"
        },
        {
          "name": "payments:process",
          "description": "Process payment transactions"
        },
        {
          "name": "payments:refund",
          "description": "Process refunds"
        }
      ],
      "orders-api": [
        {
          "name": "orders:read:own",
          "description": "Read own orders"
        },
        {
          "name": "orders:write:own",
          "description": "Create/modify own orders"
        },
        {
          "name": "orders:read:all",
          "description": "Read all orders (employees only)"
        },
        {
          "name": "orders:write:all",
          "description": "Modify any order"
        },
        {
          "name": "orders:cancel",
          "description": "Cancel orders"
        },
        {
          "name": "orders:fulfill",
          "description": "Mark orders as fulfilled"
        }
      ],
      "inventory-api": [
        {
          "name": "inventory:read",
          "description": "View inventory levels"
        },
        {
          "name": "inventory:write",
          "description": "Modify inventory levels"
        },
        {
          "name": "inventory:transfer",
          "description": "Transfer inventory between locations"
        },
        {
          "name": "inventory:adjust",
          "description": "Make inventory adjustments"
        }
      ],
      "analytics-api": [
        {
          "name": "analytics:read",
          "description": "View analytics dashboards"
        },
        {
          "name": "analytics:reports:read",
          "description": "View reports"
        },
        {
          "name": "analytics:reports:write",
          "description": "Create and manage reports"
        },
        {
          "name": "analytics:query",
          "description": "Execute analytics queries"
        }
      ],
      "product-catalog-api": [
        {
          "name": "products:read",
          "description": "View products"
        },
        {
          "name": "products:write",
          "description": "Create and modify products"
        },
        {
          "name": "products:pricing",
          "description": "Manage product pricing"
        }
      ],
      "support-api": [
        {
          "name": "tickets:read:own",
          "description": "Read own support tickets"
        },
        {
          "name": "tickets:write:own",
          "description": "Create/update own tickets"
        },
        {
          "name": "tickets:read:all",
          "description": "Read all support tickets"
        },
        {
          "name": "tickets:write:all",
          "description": "Manage all tickets"
